{
  "manifest_version": 3,
  "name": "Multi Search Extension",
  "version": "1.0.0",
  "description": "A Chrome extension that can search using multiple search engines at once.",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["activeTab", "storage"]
}
